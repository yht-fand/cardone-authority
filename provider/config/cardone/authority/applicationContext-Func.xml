<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">
    <bean id="top.cardone.authority.func.ReadListPermissionCodeFunc"
          class="top.cardone.authority.func.ReadListPermissionCodeFunc"/>

    <alias name="top.cardone.authority.func.ReadListPermissionCodeFunc" alias="readListPermissionCodeFunc"/>

    <bean id="top.cardone.authority.func.ReadListRoleCodeFunc"
          class="top.cardone.authority.func.ReadListRoleCodeFunc"/>

    <alias name="top.cardone.authority.func.ReadListRoleCodeFunc" alias="readListRoleCodeFunc"/>

    <bean id="generateUserGroupTreeFunc"
          class="top.cardone.data.func.GenerateTreeFunc">
        <property name="codePutKey" value="userGroupCode"/>
        <property name="codeGetKey" value="USER_GROUP_CODE"/>
        <property name="pkPutKey" value="userGroupId"/>
        <property name="pkGetKey" value="USER_GROUP_ID"/>
        <property name="service" ref="top.cardone.authority.service.UserGroupService"/>
    </bean>

    <bean id="generateRoleTreeFunc"
          class="top.cardone.data.func.GenerateTreeFunc">
        <property name="codePutKey" value="roleCode"/>
        <property name="codeGetKey" value="ROLE_CODE"/>
        <property name="pkPutKey" value="roleId"/>
        <property name="pkGetKey" value="ROLE_ID"/>
        <property name="service" ref="top.cardone.authority.service.RoleService"/>
    </bean>

    <bean id="generatePermissionTreeFunc"
          class="top.cardone.data.func.GenerateTreeFunc">
        <property name="codePutKey" value="permissionCode"/>
        <property name="codeGetKey" value="PERMISSION_CODE"/>
        <property name="pkPutKey" value="permissionId"/>
        <property name="pkGetKey" value="PERMISSION_ID"/>
        <property name="service" ref="top.cardone.authority.service.PermissionService"/>
    </bean>
</beans>